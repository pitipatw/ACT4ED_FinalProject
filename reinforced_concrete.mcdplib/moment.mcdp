mcdp {
    provides moment_demand [N*mm]
    
    requires section_width [mm]
    requires compression_depth [mm]
    requires concrete_strength [N/mm/mm]
    requires total_rebar_area [mm*mm]
    requires moment_arm [mm]

    fy = 420 [N/mm/mm]
    
    provided moment_demand <= 0.85 * required concrete_strength * required section_width * required compression_depth * required moment_arm
    provided moment_demand <= required total_rebar_area * fy * required moment_arm
}

