using Makie, GLMakie
include("../src/myMCDP.jl")


r_opt, r_pes = myMCDP.load_results("out-query/output.yaml")

f2 = Figure(size = (800, 800))
ax2 = Axis(f2[1, 1])
scatter!(ax2, r_opt[1], r_opt[2], color = :green)
scatter!(ax2, r_pes[1], r_pes[2], color = :red)

println("Opt")
moment_demand = r_opt[1] .* r_opt[2].^2/8
shear_demand = r_opt[1] .* r_opt[2] / 2

println("Pes")
moment_demand = r_pes[1] .* r_pes[2].^2/8
shear_demand = r_pes[1] .* r_pes[2] / 2